            <authors>
               <author>
                  <id>5158478</id>
                  <name>Anonymous</name>
                  <image_url><![CDATA[https://s.gr-assets.com/assets/nophoto/user/u_200x266-2201d4a48ef4852b422c278952e03be9.png]]></image_url>
