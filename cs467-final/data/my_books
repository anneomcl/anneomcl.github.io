            <image_url>https://d.gr-assets.com/books/1407080901m/46170.jpg</image_url>
            <small_image_url>https://d.gr-assets.com/books/1407080901s/46170.jpg</small_image_url>
            <large_image_url />
            <link>https://www.goodreads.com/book/show/46170.For_Whom_the_Bell_Tolls</link>
            <num_pages>471</num_pages>
            <format>Paperback</format>
