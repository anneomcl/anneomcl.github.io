            <title>Middlemarch</title>
            <image_url>https://s.gr-assets.com/assets/nophoto/book/111x148-c93ac9cca649f584bf7c2539d88327a8.png</image_url>
            <small_image_url>https://s.gr-assets.com/assets/nophoto/book/50x75-4845f44723bc5d3a9ac322f99b110b1d.png</small_image_url>
            <large_image_url />
            <link>https://www.goodreads.com/book/show/19089.Middlemarch</link>
            <num_pages>904</num_pages>
            <format>Paperback</format>
            <edition_information />
            <publisher>Signet</publisher>
            <publication_day />
            <publication_year>2004</publication_year>
            <publication_month />
            <average_rating>3.89</average_rating>
